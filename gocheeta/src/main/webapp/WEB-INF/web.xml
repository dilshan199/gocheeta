<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CustomerServlet</servlet-name>
  	<display-name>CustomerServlet</display-name>
  	<description>This will handle customer  related operation</description>
  	<servlet-class>com.gocheeta.web.controller.CustomerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>VehicleServlet</servlet-name>
  	<display-name>VehicleServlet</display-name>
  	<description>This will handle all the vehicle related operation</description>
  	<servlet-class>com.gocheeta.web.controller.VehicleServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>HomeServlet</servlet-name>
  	<display-name>HomeServlet</display-name>
  	<description>This will handle all the home related operation</description>
  	<servlet-class>com.gocheeta.web.controller.HomeServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookingServlet</servlet-name>
  	<display-name>BookingServlet</display-name>
  	<description>This will handle the customer booking</description>
  	<servlet-class>com.gocheeta.web.controller.BookingServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LocationServlet</servlet-name>
  	<display-name>LocationServlet</display-name>
  	<description>Get all location related branch</description>
  	<servlet-class>com.gocheeta.web.controller.LocationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CityServlet</servlet-name>
  	<display-name>CityServlet</display-name>
  	<description>Get all cities related branch</description>
  	<servlet-class>com.gocheeta.web.controller.CityServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DestinationServlet</servlet-name>
  	<display-name>DestinationServlet</display-name>
  	<description>This servlet get destination related between street and city</description>
  	<servlet-class>com.gocheeta.web.controller.DestinationServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description>This will handle the login operation</description>
  	<servlet-class>com.gocheeta.web.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProfileServlet</servlet-name>
  	<display-name>ProfileServlet</display-name>
  	<description>Customer profile related operation</description>
  	<servlet-class>com.gocheeta.web.controller.ProfileServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FeedbackServlet</servlet-name>
  	<display-name>FeedbackServlet</display-name>
  	<description>This will handle the feedback operation</description>
  	<servlet-class>com.gocheeta.web.controller.FeedbackServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DLoginServlet</servlet-name>
  	<display-name>DLoginServlet</display-name>
  	<description>Driver Login Operations</description>
  	<servlet-class>com.gocheeta.web.controller.DLoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DriverProfileServlet</servlet-name>
  	<display-name>DriverProfileServlet</display-name>
  	<description>This is driver profile operation</description>
  	<servlet-class>com.gocheeta.web.controller.DriverProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CustomerServlet</servlet-name>
  	<url-pattern>/customer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VehicleServlet</servlet-name>
  	<url-pattern>/vehicle</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>HomeServlet</servlet-name>
  	<url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookingServlet</servlet-name>
  	<url-pattern>/placebooking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LocationServlet</servlet-name>
  	<url-pattern>/LocationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CityServlet</servlet-name>
  	<url-pattern>/CityServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DestinationServlet</servlet-name>
  	<url-pattern>/DestinationServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProfileServlet</servlet-name>
  	<url-pattern>/profile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FeedbackServlet</servlet-name>
  	<url-pattern>/feedback</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DLoginServlet</servlet-name>
  	<url-pattern>/driverlogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DLoginServlet</servlet-name>
  	<url-pattern>/driverlogout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverProfileServlet</servlet-name>
  	<url-pattern>/driverprofile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverProfileServlet</servlet-name>
  	<url-pattern>/readmore</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverProfileServlet</servlet-name>
  	<url-pattern>/confirm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverProfileServlet</servlet-name>
  	<url-pattern>/complete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverProfileServlet</servlet-name>
  	<url-pattern>/cancel</url-pattern>
  </servlet-mapping>
</web-app>
